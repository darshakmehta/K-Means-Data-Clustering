  <section id="filter">
    <div class="container">
      <div class="row">
        <div class="col-md-12"><hr></div>
        <div class="col-md-3 col-sm-3 col-lg-3 col-xs-3 text-center filters"><span class="glyphicon glyphicon-triangle-bottom"></span><br><span>RATINGS</span></div>
        <div class="col-md-3 col-sm-3 col-lg-3 col-xs-3 text-center filters"><span class="glyphicon glyphicon-triangle-bottom"></span><br><span>HIGH PRICE</span></div>
        <div class="col-md-3 col-sm-3 col-lg-3 col-xs-3 text-center filters"><span class="glyphicon glyphicon-triangle-bottom"></span><br><span>LOW PRICE</span></div>
        <div class="col-md-3 col-sm-3 col-lg-3 'col-xs-3 text-center">
          <div class="form-group">
            <input type="text" placeholder="search" class="form-control" ng-model="search">
          </div>
        </div>
      </div>
    </div> 
  </section>

  
  <section id="product">
    <div class="container" ng-controller="mainController">
      

      <div class="row">
                    <div class="col-lg-3 col-md-4 col-sm-6" ng-repeat="products in Cluster | filter:search">
                              <div class="thumbnail">
                                            <img ng-src="{{products.img_url}}" alt="..."/>
                                 
                              <div class="caption">
                                    <h3>{{products.name}}</h3>
                                    <p>{{products.price | currency:"&#8377"}}</p>
                        <p>Ratings:{{products.average_rating}}
                          <div ng-switch on ="products.rating">
                            <div ng-switch-when="1">
                              <div class="glyphicon glyphicon-star" style="color:yellow;cursor:pointer" ng-click="updateRating($index,products_id,1)"></div>
                              <div class="glyphicon glyphicon-star" style="cursor:pointer" ng-click="updateRating($index,products_id,2)"></div>
                              <div class="glyphicon glyphicon-star" style="cursor:pointer" ng-click="updateRating($index,products_id,3)"></div>
                              <div class="glyphicon glyphicon-star" style="cursor:pointer" ng-click="updateRating($index,products_id,4)"></div>
                              <div class="glyphicon glyphicon-star" style="cursor:pointer" ng-click="updateRating($index,products_id,5)"></div>
                            </div>
                            <div ng-switch-when="2">
                              <div class="glyphicon glyphicon-star" style="color:yellow;cursor:pointer" ng-click="updateRating($index,products_id,1)"></div>
                              <div class="glyphicon glyphicon-star" style="color:yellow;cursor:pointer" ng-click="updateRating($index,products_id,2)"></div>
                              <div class="glyphicon glyphicon-star" style="cursor:pointer" ng-click="updateRating($index,products_id,3)"></div>
                              <div class="glyphicon glyphicon-star" style="cursor:pointer" ng-click="updateRating($index,products_id,4)"></div>
                              <div class="glyphicon glyphicon-star" style="cursor:pointer" ng-click="updateRating($index,products_id,5)"></div>
                            </div>
                            <div ng-switch-when="3">
                              <div class="glyphicon glyphicon-star" style="color:yellow;cursor:pointer" ng-click="updateRating($index,products_id,1)"></div>
                              <div class="glyphicon glyphicon-star" style="color:yellow;cursor:pointer" ng-click="updateRating($index,products_id,2)"></div>
                              <div class="glyphicon glyphicon-star" style="color:yellow;cursor:pointer" ng-click="updateRating($index,products_id,3)"></div>
                              <div class="glyphicon glyphicon-star" style="cursor:pointer" ng-click="updateRating($index,products_id,4)"></div>
                              <div class="glyphicon glyphicon-star" style="cursor:pointer" ng-click="updateRating($index,products_id,5)"></div>
                            </div>
                            <div ng-switch-when="4">
                              <div class="glyphicon glyphicon-star" style="color:yellow;cursor:pointer" ng-click="updateRating($index,products_id,1)"></div>
                              <div class="glyphicon glyphicon-star" style="color:yellow;cursor:pointer" ng-click="updateRating($index,products_id,2)"></div>
                              <div class="glyphicon glyphicon-star" style="color:yellow;cursor:pointer" ng-click="updateRating($index,products_id,3)"></div>
                              <div class="glyphicon glyphicon-star" style="color:yellow;cursor:pointer" ng-click="updateRating($index,products_id,4)"></div>
                              <div class="glyphicon glyphicon-star" style="cursor:pointer" ng-click="updateRating($index,products_id,5)"></div>
                            </div>
                            <div ng-switch-when="5">
                              <div class="glyphicon glyphicon-star" style="color:yellow;cursor:pointer" ng-click="updateRating($index,products_id,1)"></div>
                              <div class="glyphicon glyphicon-star" style="color:yellow;cursor:pointer" ng-click="updateRating($index,products_id,2)"></div>
